{
  "name": "backend-app",
  "version": "0.0.0",
  "scripts": {
    "start": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "seed": "ts-node src/cli/seed.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "jest": "jest --passWithNoTests",
    "test": "npm run typecheck && npm run jest"
  },
  "dependencies": {
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^2.6.7",
    "runtypes": "^6.6.0",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/node-fetch": "^2.6.2",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true
}
